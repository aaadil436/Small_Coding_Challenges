# Check if a number is an armstrong number
# An armstrong number is the one if the sum of each digit raised to the power number of digits equals to the number itself i.e. 371= 3^3 + 7^3 +1^3.


def armstrong(n: int) -> bool:
    

    a=n
    u=n

    arm=0
    total_digits=0
    j=0
    while j>=0:
        a=a/10
        j=j+1
        if a<1:
            total_digits=j

            break

    for j in range(total_digits):

        b=u%10
        u=(u-b)/10

        arm=arm+b**total_digits

        if u<10:
            arm=arm+u**total_digits
            arm=int(arm)
            break
    if n==arm:
        return True
    else:
        return False

            
            
    
    return bool
    

    
if __name__=='__main__':
    p=int(input())
    armstrong(p)
    if armstrong(p):
        print("True")
    else:
        print("False")
        

# Sample Input: 371   
# Sample Output: True

# Sample Input: 666   
# Sample Output: False
        
    
    
